{
  "title": "Change right-cmd and right-opt to...",
  "maintainers": [
    "halfwhole"
  ],
  "rules": [
    {
      "description": "Change right cmd to cmd + tab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_command"
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["left_command"]
            }
          ],
          "conditions": [
            {
              "type": "keyboard_type_if",
              "keyboard_types": [
                "jis"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Change right opt to cmd + f1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option"
          },
          "to": [
            {
              "key_code": "f1",
              "modifiers": ["left_command"]
            }
          ],
          "conditions": [
            {
              "type": "keyboard_type_if",
              "keyboard_types": [
                "jis"
              ]
            }
          ]
        }
      ]
    }
  ]
}
